{
  "TargetExtensions": [
    ".cs"
  ],
  "CommentAndIgnoreRegexp": [
    "\/\/(?<comment>[^\\n\\r]*)",
    "\/\\*(?<comment>(.|\\n)*?)\\*\/",
    "#(.|\\n)*?(?<!\\\\)(?=\\r|\\n)",
    "\"(.|\\n)*?(?<!\\\\)\""
  ],
  "FunctionBeginMarker": "{",
  "FunctionEndMarker": "}",
  "FunctionRegexp": [
    "(?![^{]*(?<!\\boperator\\s*)=)(?=[^{]*\\b\\w+\\b[^{]*\\()\\b\\w[^;}{]+(?<!\\b(for|if|else|while|foreach|switch|using|new)\\b[^;}{]*)(?={)"
  ],
  "FunctionCheckerInfos": [
    {
      "FunctionCheckerName": "BeginEndPairChecker",
      "Args": [ "(", ")" ]
    }
  ]
}
